{
  "name": "geschichten",
  "version": "1.0.0",
  "description": "App for creating public and private stories",
  "main": "app.js",
  "scripts": {
    "build": "cd client && npm run build",
    "install-client": " cd client npm install",
    "heroku-postbuild": "npm run install-client && npm run build",
    "start": "node server",
    "server": "nodemon server",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Gbenimako",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "install": "^0.13.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.27.0",
    "mongoose": "^5.9.19",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "nodemon": "^2.0.4"
  }
}
